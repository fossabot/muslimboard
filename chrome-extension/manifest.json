{
  "manifest_version": 2,
  "name": "Muslim Board",
  "short_name": "muslimboard",
  "version": "2.3.6.4",
  "description": "Personal Dashboard for Muslim",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "icons": {
    "16": "assets/internal/images/logo-16.jpg",
    "48": "assets/internal/images/logo-48.jpg",
    "128": "assets/internal/images/logo-128.jpg"
  },
  "permissions": [
    "geolocation",
    "alarms",
    "notifications",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://bimasislam.kemenag.go.id https://unsplash.com; object-src 'self'"
}
  